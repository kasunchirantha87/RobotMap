
@{
    ViewData["Title"] = "Upload";
}

<h2>Upload</h2>

<form method="post" enctype="multipart/form-data" asp-controller="Path" asp-action="UploadImage">
    <div class="row">
        <div class="form-group">
            <div class="col-md-10">
                <input type="file" name="file" multiple />
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Upload" />
            </div>
        </div>
    </div>
</form>
